{
  "name": "figma-api",
  "version": "1.11.0",
  "description": "Figma js typed api",
  "main": "src/index.js",
  "types": "src/index.d.ts",
  "scripts": {
    "build": "tsc && npm run-script browserify && npm run-script uglify",
    "browserify": "browserify src/index.js --standalone Figma -o src/figma-api.js",
    "uglify": "browserify -g [ uglifyify ] src/index.js --standalone Figma -o src/figma-api.min.js"
  },
  "keywords": [
    "figma",
    "figmajs",
    "json",
    "api",
    "parser",
    "ast",
    "typed"
  ],
  "author": "morglod",
  "license": "MIT",
  "dependencies": {
    "@types/node": "12.0.2",
    "axios": "^0.21.1",
    "typescript": "3.4.5"
  },
  "devDependencies": {
    "browserify": "16.2.3",
    "uglifyify": "^5.0.1"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/didoo/figma-api"
  }
}
